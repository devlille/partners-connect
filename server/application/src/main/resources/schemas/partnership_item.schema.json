    },
    "company_name": {
      "type": "string"
    },
    "event_name": {
      "type": "string"
    },
    "pack_name": {
      "type": "string",
      "nullable": true
    },
    "suggested_pack_name": {
      "type": "string",
      "nullable": true
    },
    "language": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "nullable": true
    },
    "emails": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
