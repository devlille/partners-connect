    "nuxt": "^4.2.0",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.20.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@vitest/ui": "^4.0.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.10",
    "oxlint": "^1.24.0",
