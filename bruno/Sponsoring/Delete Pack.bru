  org_slug: {{org_slug}}
  pack_id: {{pack_id}}
  event_id: {{event_id}}
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "name": "Gold",
    "price": 9000,
    "max_quantity": 20
  }
}
