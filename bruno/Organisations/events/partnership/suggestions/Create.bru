meta {
  name: Create
  type: http
  seq: 1
}

post {
  url: {{scheme}}://{{base_url}}/orgs/:org_slug/events/:event_slug/partnerships/:partnership_id/suggestion
  body: json
  auth: none
}

params:path {
  event_slug: {{event_slug}}
  org_slug: {{org_slug}}
  partnership_id: {{partnership_id}}
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "pack_id": "{{pack_id}}",
    "option_selections": [],
    "language": "en"
  }
}
