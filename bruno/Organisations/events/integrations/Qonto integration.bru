meta {
  name: Qonto integration
  type: http
  seq: 3
}

post {
  url: {{scheme}}://{{base_url}}/orgs/:org_slug/events/:event_slug/integrations/qonto/billing
  body: json
  auth: none
}

params:path {
  event_slug: {{event_slug}}
  org_slug: {{org_slug}}
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "api_key": "{{qonto_api_key}}",
    "secret": "{{qonto_secret}}",
    "sandbox_token": "{{qonto_sandbox}}"
  }
}
