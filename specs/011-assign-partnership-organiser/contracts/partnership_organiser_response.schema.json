{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["partnership_id", "organiser"],
  "properties": {
    "partnership_id": {
      "type": "string",
      "format": "uuid",
      "description": "UUID of the partnership"
    },
    "organiser": {
      "oneOf": [
        {
          "$ref": "user.schema.json"
        },
        {
          "type": "null"
        }
      ],
      "description": "Information about the assigned organiser (User object), or null if no organiser is assigned"
    }
  },
  "additionalProperties": false
}
